generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model File {
  id           String   @id
  content_type String
  file_name    String
  file_size    String
  file_path    String
  is_picture   Boolean
  status       Boolean?
  // users        User[]
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt

  @@map("File")
}